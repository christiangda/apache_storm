{
  "name": "christiangda-apache_storm",
  "version": "0.0.1",
  "author": "Christian Gonzalez <christiangda@gmail.com>",
  "summary": "Apache Storm provisioner Module",
  "license": "Apache-2.0",
  "source": "https://github.com/christiangda/apache_storm",
  "project_page": "https://github.com/christiangda/apache_storm",
  "issues_url": "https://github.com/christiangda/apache_storm/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.7.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7.0",
        "8.0"
      ]
    }
  ]
}
